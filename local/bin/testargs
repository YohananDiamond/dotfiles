#!/usr/bin/env sh

# This script shows info about arguments passed to it.
# It's useful for inspecting how word splitting works in your shell.

printf "Argument count: " >/dev/stderr
echo $#
echo "Arguments: " >/dev/stderr
while [ $# != 0 ]; do
  printf "  " >/dev/stderr
  printf "$1\n"
  shift
done
echo "[END]" >/dev/stderr
